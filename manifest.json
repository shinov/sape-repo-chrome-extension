{
    "manifest_version": 2,

    "name": "Sapient Repos",
    "description": "A chrome extension for pulling GitHub Repos contributed to Sapient repo Code base",
    "version": "1.0.1",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "anonymous.html"
    },

    "background": {
        "scripts": ["jquery-1.11.1.min.js","utilities.js"],
        "persistent": false
    },

    "permissions": [
        "https://api.github.com/",
        "storage",
        "notifications",
        "alarms"
    ]
}
